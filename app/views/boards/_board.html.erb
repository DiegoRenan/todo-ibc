<div class="col-xs-4 col-md-3">
  <div class="card card-board">
    <div class="d-flex flex-row-reverse">
      <%= render 'shared/menus', controller: 'boards', id: board %>
      <%= link_to board do %>
        <div class="card-body">
          <h5 class="card-title">
            <%= board.title %>
          </h5>
        </div>
      <% end %>
    </div>
  </div>
</div>